<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox minWidth="320.0" prefHeight="720.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 36px;" text="Chat">
         <font>
            <Font name="Comic Sans MS" size="36.0" />
         </font>
         <VBox.margin>
            <Insets left="24.0" top="24.0" />
         </VBox.margin>
      </Text>
      <TextField fx:id="search" accessibleRole="BUTTON" editable="false" onAction="#onSearchNewUser" style="-fx-background-color: #f8f8fa; -fx-border-radius: 50px; -fx-border-width: 1px; -fx-border-color: white; -fx-background-radius: 50px; -fx-background-image: url('https://www.iconpacks.net/icons/2/free-search-icon-3076-thumb.png'); -fx-background-position: right center; -fx-background-size: 20px; -fx-background-repeat: no-repeat;">
         <padding>
            <Insets bottom="8.0" left="16.0" right="16.0" top="8.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
      </TextField>
      <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" stylesheets="@../styles/tab.css" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab fx:id="chat" closable="false" text="chat">
               <content>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="566.0" prefWidth="320.0" style="-fx-background-color: #ffffff;">
                           <content>
                              <VBox prefWidth="277.0" style="-fx-background-color: white;">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 16px;" text="Messages" VBox.vgrow="ALWAYS">
                                       <font>
                                          <Font name="Comic Sans MS" size="16.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets left="8.0" />
                                       </VBox.margin>
                                    </Text>
                                    <VBox fx:id="chatList" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </content>
                        </ScrollPane>
                     </children>
                  </VBox>
               </content>
            </Tab>
          <Tab fx:id="conv" text="conv">
               <content>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="36.0" prefWidth="320.0" style="-fx-background-color: #ffffff;">
                           <content>
                              <VBox prefHeight="452.0" prefWidth="318.0" style="-fx-background-color: white;">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 16px;" text="Messages" VBox.vgrow="ALWAYS">
                                       <font>
                                          <Font name="Comic Sans MS" size="16.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets left="8.0" />
                                       </VBox.margin>
                                    </Text>
                                    <VBox fx:id="chatList1" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </content>
                        </ScrollPane>
                     </children>
                  </VBox>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</VBox>
